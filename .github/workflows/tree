#include <iostream>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */


int main(int argc, char** argv) {  

const int n=10;
int t [n];
 

 for (int i=0;i<=n;i++){
 	std::cout<<"donner le tablo t["<< i <<"]";
 	std::cin>> t [i];
 }
 for (int i=2;i<=n;i++){ 
 int a = t[i];
 int j =i ;
 
 while ( j>1 and t[j-1]>a){ 
t[j]=t[j-1];
j--;
 }
 t[j]=a;

 }
 for  (int i=1;i<=n;i++){
std:: cout <<"le tableu tri a selection t["<<i<<"]"<<t[i];
}
 	return 0;

 }
